{
    "displayName": "AI Playground",
    "name": "ai-playground",
    "scope": "public",
    "version": "0.0.21",
    "attributes": {
        "Provider": "Dataloop",
        "deployed By": "Dataloop",
        "Category": "Application",
        "Application Type": "Pipeline Node",
        "Node Type": "Integration"
    },
    "codebase": {
        "type": "git",
        "gitUrl": "https://github.com/dataloop-ai-apps/ai-playground",
        "gitTag": "0.0.21"
    },
    "components": {
        "panels": [
            {
                "name": "ai",
                "supportedSlots": [
                    {
                        "type": "aiPlayground"
                    }
                ],
                "conditions": {
                    "resources": []
                }
            }
        ],
        "modules": [
            {
                "name": "ai_chat_module",
                "entryPoint": "main.py",
                "className": "Runner",
                "config": {
                    "isCustomServer": true
                },
                "initInputs": [],
                "functions": [
                    {
                        "name": "run",
                        "input": [],
                        "output": [],
                        "displayIcon": "icon-dl-dataloop",
                        "displayName": "AI Chat"
                    }
                ]
            }
        ],
        "services": [
            {
                "name": "ai-chat-service",
                "moduleName": "ai_chat_module",
                "botUserName": "tab-apps@dataloop.ai",
                "panelNames": ["ai"],
                "runtime": {
                    "runnerImage": "gcr.io/viewo-g/piper/agent/cpu/ingest-gradio:0.1.15",
                    "podType": "regular-m",
                    "concurrency": 10,
                    "autoscaler": {
                        "type": "rps",
                        "cooldownPeriod": 1800,
                        "threshold": 1,
                        "rateSeconds": 30,
                        "minReplicas": 1,
                        "maxReplicas": 1,
                        "queueLength": 10
                    }
                }
            }
        ]
    }
}
